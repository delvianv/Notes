{
  "expo": {
    "name": "Notes",
    "description": "Quickly take notes with ease",
    "slug": "Notes",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "notes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffbff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b680e21f-347a-4afe-9081-177ff043e555"
      }
    },
    "android": {
      "package": "yocto.Notes"
    }
  }
}
